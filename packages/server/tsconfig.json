{ // ps -efx then kill pid
    "compilerOptions": {
      "target": "es6",
      "module": "NodeNext",
      "baseUrl": "./src",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "NodeNext",
      "forceConsistentCasingInFileNames": true,
      // "skipLibCheck": true,
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"],
    "lib": ["esnext"]
}