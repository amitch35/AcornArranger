    "cross-env": "^7.0.3",
    "esbuild": "^0.20.2",
    "esbuild-node-tsc": "^2.0.5",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.43.1",
    "dotenv": "^16.4.5",
