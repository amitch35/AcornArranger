  "main": "dist/indext.js",
  "exports": {
    "./models": {
      "import": "./src/models/index.ts"
    }
  },
  "scripts": {
    "dev": "cross-env STATIC=../proto/public npx nodemon",
    "build": "npx etsc",
    "start": "npm run build && npm run start:node",
